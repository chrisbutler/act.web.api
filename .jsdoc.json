{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["act.web.api.js", "README.md"],
    "excludePattern": "(node_modules/|docs/)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true
  },
  "opts": {
    "destination": "./docs",
    "encoding": "utf8",
    "private": true,
    "recuse": true,
    "template": "./node_modules/minami"
  }
}
